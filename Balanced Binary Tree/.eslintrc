{
    "extends": [
        "eslint:recommended"
    ],
    "env": {
        "es6": true,
        "node": true,
        "browser": false
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules":{
        "no-console": "off",
        "no-unused-vars": "warn"
    }
}